	//-----SQL문 변수 초기화 함수-----
	private void initSQLConst(){
		sql_Query = "SELECT (mc.major_code||sc.minor_code) as gubun, min(mc.mcode_name)||min(sc.scode_name)||'교육' as gubunnm, count(distinct a.grcode) grcode_cnt,  nvl(sum(a.tseat),0) plan_inwon, count(a.grcode) grseq_cnt, nvl(sum(c.totno),0) resu_inwon ";
		sql_Query = sql_Query + "FROM tb_grseq a, tb_grcode gc, tb_maincode mc, tb_subcode sc, ";
		sql_Query = sql_Query + " (select grcode,grseq ,sum(decode('" + p_dept_Param + "', null, 1, '', 1,  rdept, 1, 0)) totno ";
		sql_Query = sql_Query + " from TB_GRRESULT group by grcode,grseq) c ";
		sql_Query = sql_Query + "WHERE to_char(a.enddate, 'yyyymm') between '" + p_startmonth_Param + "' ";
		sql_Query = sql_Query + "and '" + p_endmonth_Param + "' ";
		sql_Query = sql_Query + "and a.grcode=c.grcode(+) ";
		sql_Query = sql_Query + "and a.grseq=c.grseq(+) ";
		sql_Query = sql_Query + "and a.grcode = gc.grcode ";
		sql_Query = sql_Query + "and mc.cd_gubun='G' ";
		sql_Query = sql_Query + "and gc.grtype=mc.major_code ";
		sql_Query = sql_Query + "and sc.cd_gubun='G' ";
		sql_Query = sql_Query + "and gc.grtype=sc.major_code ";
		sql_Query = sql_Query + "and gc.grsubcd=sc.minor_code ";
		sql_Query = sql_Query + "GROUP BY (mc.major_code||sc.minor_code)";
	}

