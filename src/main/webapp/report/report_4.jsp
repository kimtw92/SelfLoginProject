	private void getScriptFromloop(boolean bIsPrint) throws Exception{
		//지역 변수 선언 및 초기화
		int nBandHeight=30;    //밴드 출력 높이
		boolean bIsFirst=true;
		boolean bIsTrue_item=false;   //item 조건 밴드 플래그 선언

