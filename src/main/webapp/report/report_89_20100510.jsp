		sql_LIST = sql_LIST + "        CASE WHEN SUM(CASE WHEN QUESTION_TYPE = 2 THEN SUB_CNT1 + SUB_CNT2 + SUB_CNT3 + SUB_CNT4 + SUB_CNT5 ELSE 0 END) =0 THEN 0 ELSE ROUND(SUM(CASE WHEN QUESTION_TYPE = 2 THEN SUB_CNT1 + SUB_CNT2 ELSE 0 END) / SUM(CASE WHEN QUESTION_TYPE = 2 THEN SUB_CNT1 + SUB_CNT2 + SUB_CNT3 + SUB_CNT4 + SUB_CNT5 ELSE 0 END)*100,1) END AS STIME_ANS11_RAT, ";
		sql_LIST = sql_LIST + "        CASE WHEN SUM(CASE WHEN QUESTION_TYPE = 2 THEN SUB_CNT1 + SUB_CNT2 + SUB_CNT3 + SUB_CNT4 + SUB_CNT5 ELSE 0 END) =0 THEN 0 ELSE ROUND(SUM(CASE WHEN QUESTION_TYPE = 2 THEN SUB_CNT3 ELSE 0 END) / SUM(CASE WHEN QUESTION_TYPE = 2 THEN SUB_CNT1 + SUB_CNT2 + SUB_CNT3 + SUB_CNT4 + SUB_CNT5 ELSE 0 END)*100,1) END AS STIME_ANS22_RAT, ";
