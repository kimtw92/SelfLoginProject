		sql_CATEGORY = "SELECT DISTINCT CATEGORY FROM TB_GRSUGGESTION WHERE FINISH_YN = 'Y'";
		sql_QUESTION = "select ";
		sql_QUESTION = sql_QUESTION + "    a.question_no as question_no, ";
		sql_QUESTION = sql_QUESTION + "    min(a.question) as question, ";
