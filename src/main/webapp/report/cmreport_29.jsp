	//-----SQL문 변수 초기화 함수-----
	private void initSQLConst(){
		sql_M = "SELECT partnm, jiknm, name  FROM cmtv_unmember WHERE userno='" + p_userno_Param + "'";
		sql_L1 = "SELECT gs.grcodeniknm, ai.grcode, ai.grseq , sj.subj, to_char(gs.started, 'yyyy.mm.dd') || '~' ";
		sql_L1 = sql_L1 + "        || to_char(gs.enddate, 'yyyy.mm.dd') period,  sj.lecnm , nvl(CMProgressSubj(sj.grcode, sj.grseq, sj.subj,  ";
		sql_L1 = sql_L1 + "         ai.userno),0) as cur_paccept ,  ";
		sql_L1 = sql_L1 + "         case when to_char(sysdate, 'yyyymmdd') >= to_char(gs.started, 'yyyymmdd') and  ";
		sql_L1 = sql_L1 + "                       to_char(sysdate, 'yyyymmdd') <= to_char(gs.enddate, 'yyyymmdd') then '교육진행중'  ";
		sql_L1 = sql_L1 + "         when to_char(sysdate, 'yyyymmdd') > to_char(gs.enddate, 'yyyymmdd') and (gs.closing='N' or gs.closing is null) then '심사중'  ";
		sql_L1 = sql_L1 + "         when to_char(sysdate, 'yyyymmdd') > to_char(gs.enddate, 'yyyymmdd') and gs.closing='Y'  ";
		sql_L1 = sql_L1 + "                  then decode(gr.rgrayn,'Y','수료', 'N', '미수료')  end as txt_state  ";
		sql_L1 = sql_L1 + "FROM cmtv_unapp_info ai, cmtb_subjseq sj, cmtb_grseq gs, cmtv_ungrresult gr ";
		sql_L1 = sql_L1 + "WHERE ai.grcode=gs.grcode and ai.grseq=gs.grseq and ai.grcode=sj.grcode and ai.grseq=sj.grseq ";
		sql_L1 = sql_L1 + "            and ai.grcode=gr.grcode(+) and ai.grseq=gr.grseq(+) and ai.userno=gr.userno(+) ";
		sql_L1 = sql_L1 + "            and ai.userno='" + p_userno_Param + "' and ai.grchk='Y' and ai.cancel_dt is null ";
		sql_L1 = sql_L1 + "ORDER BY sj.subj";
	}

