				grcodenm = stat_grcodenm.executeQuery(sql_grcodenm);
			}else{
				grcodenm.beforeFirst();
			}

			//데이터 fetch
			if(grcodenm.next()){
				setFieldVariableFromgrcodenm();	//ResultSet grcodenm에서 필드 값 설정
			}else{
				setFieldInitFromgrcodenm();	//ResultSet grcodenm의 필드 값 초기화
