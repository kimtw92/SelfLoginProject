	//-----SQL문 변수 초기화 함수-----
	private void initSQLConst(){
		sql_totSum = "SELECT SUM(a.grseq_cnt) AS grseq_cnt ";
		sql_totSum = sql_totSum + "     , SUM(a.plan_inwon) AS plan_inwon ";
		sql_totSum = sql_totSum + "     , SUM(a.grseq_resucnt) AS grseq_resucnt ";
		sql_totSum = sql_totSum + "     , SUM(a.man_resucnt) AS man_resucnt ";
		sql_totSum = sql_totSum + "     , SUM(a.wo_resucnt) AS wo_resucnt ";
		sql_totSum = sql_totSum + "     , SUM(a.resu_inwon) AS resu_inwon ";
		sql_totSum = sql_totSum + "  FROM ( ";
		sql_totSum = sql_totSum + "SELECT (gc.grtype || gc.grsubcd) as gubun ";
		sql_totSum = sql_totSum + "     ,(min(mc.mcode_name) || min(sc.scode_name) || '교육') as gubunnm ";
		sql_totSum = sql_totSum + "     , a.grcode ";
		sql_totSum = sql_totSum + "     , min(gc.grcodenm) grcodenm ";
		sql_totSum = sql_totSum + "     , min(d.subjnm) subjnm ";
		sql_totSum = sql_totSum + "     , count(distinct a.grseq) grseq_cnt ";
		sql_totSum = sql_totSum + "     , nvl(sum(a.tseat),0) plan_inwon ";
		sql_totSum = sql_totSum + "     , count(distinct c.grseq) grseq_resucnt ";
		sql_totSum = sql_totSum + "     , nvl(sum(c.man),0) man_resucnt ";
		sql_totSum = sql_totSum + "     , nvl(sum(c.wo),0) wo_resucnt ";
		sql_totSum = sql_totSum + "     , nvl(sum(c.totno),0) resu_inwon  ";
		sql_totSum = sql_totSum + "  FROM tb_grseq a, tb_grcode gc, tb_maincode mc, tb_subcode sc, tb_subj	d ";
		sql_totSum = sql_totSum + "     , (select x.grcode ";
		sql_totSum = sql_totSum + "	     , x.grseq ";
		sql_totSum = sql_totSum + "	     , x.subj ";
		sql_totSum = sql_totSum + "	     , sum(decode(substr(x.rresno,7,1),'1',decode('" + p_dept_Param + "', null, 1, '', 1,  z.dept, 1, 0),0)) man ";
		sql_totSum = sql_totSum + "	     , sum(decode(substr(x.rresno,7,1),'2',decode('" + p_dept_Param + "', null, 1, '', 1,  z.dept, 1, 0),0)) wo ";
		sql_totSum = sql_totSum + "	     , sum(decode('" + p_dept_Param + "', null, 1, '', 1,  z.dept, 1, 0)) totno  ";
		sql_totSum = sql_totSum + "	  from tb_subjresult x,tb_subj y, tb_app_info z  ";
		sql_totSum = sql_totSum + "	 where y.subjtype='Y'  ";
		sql_totSum = sql_totSum + "	 and x.subj=y.subj  ";
		sql_totSum = sql_totSum + "	 and x.subj not in ('SUB1000025')  ";
		sql_totSum = sql_totSum + "	 and x.grcode=z.grcode(+)  ";
		sql_totSum = sql_totSum + "	 and x.grseq=z.grseq(+)  ";
		sql_totSum = sql_totSum + "	 and x.userno=z.userno(+)  ";
		sql_totSum = sql_totSum + "	 group by x.grcode,x.grseq,x.subj) c  ";
		sql_totSum = sql_totSum + "  WHERE  to_char(a.enddate,'yyyymm') between '" + p_startmonth_Param + "' and '" + p_endmonth_Param + "' ";
		sql_totSum = sql_totSum + "  and a.grcode = gc.grcode  ";
		sql_totSum = sql_totSum + "  and (mc.cd_gubun(+)='G' and gc.grtype=mc.major_code(+))  ";
		sql_totSum = sql_totSum + "  and (sc.cd_gubun(+)='G' and gc.grtype=sc.major_code(+) and gc.grsubcd=sc.minor_code(+))  ";
		sql_totSum = sql_totSum + "  and a.grcode=c.grcode(+)  ";
		sql_totSum = sql_totSum + "  and a.grseq=c.grseq(+) ";
		sql_totSum = sql_totSum + "  and c.subj=d.subj ";
		sql_totSum = sql_totSum + "  GROUP BY (gc.grtype || gc.grsubcd), a.grcode,d.subj)	a";
		sql_master = "SELECT a.gubun, a.gubunnm ";
		sql_master = sql_master + "     , SUM(a.grseq_cnt) AS grseq_cnt ";
		sql_master = sql_master + "     , SUM(a.plan_inwon) AS plan_inwon ";
		sql_master = sql_master + "     , SUM(a.grseq_resucnt) AS grseq_resucnt ";
		sql_master = sql_master + "     , SUM(a.man_resucnt) AS man_resucnt ";
		sql_master = sql_master + "     , SUM(a.wo_resucnt) AS wo_resucnt ";
		sql_master = sql_master + "     , SUM(a.resu_inwon) AS resu_inwon ";
		sql_master = sql_master + "  FROM ( ";
		sql_master = sql_master + "SELECT (gc.grtype || gc.grsubcd) as gubun ";
		sql_master = sql_master + "     ,(min(mc.mcode_name) || min(sc.scode_name) || '교육') as gubunnm ";
		sql_master = sql_master + "     , a.grcode ";
		sql_master = sql_master + "     , min(gc.grcodenm) grcodenm ";
		sql_master = sql_master + "     , min(d.subjnm) subjnm ";
		sql_master = sql_master + "     , count(distinct a.grseq) grseq_cnt ";
		sql_master = sql_master + "     , nvl(sum(a.tseat),0) plan_inwon ";
		sql_master = sql_master + "     , count(distinct c.grseq) grseq_resucnt ";
		sql_master = sql_master + "     , nvl(sum(c.man),0) man_resucnt ";
